{
  "manifest_version": 2,

  "name": "StarFOSS (beta)",
  "description": "Stars all FOSSASIA repositories. Each and every one!",
  "version": "1.0",

  "icons": {
    "16": "icon.png",
	  "128": "icon.png"
  },

  "background": [ { "js": "star.user.js" } ],
  "permissions": ["tabs", "<all_urls>"],

  "web_accessible_resources": [
    "assets/fossasia.png",
    "assets/github.png",
    "assets/twitter.png",
    "assets/youtube.png",
    "assets/facebook.png",
    "assets/linkedin.png",
    "assets/join.png",
    "assets/star.png",
    "assets/natural_paper.png"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+W"
      }
    }
  }
}
